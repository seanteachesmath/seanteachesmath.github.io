knitr::opts_chunk$set(echo = TRUE)
pow  <- c(1,2, 3, 4)
plot(NULL, xlim=c(0, 4), ylim=c(0, 20))
plot(function(x) x^pow[1], xlim=c(0, 1), add=T)
for(i in 2:length(pow)){
plot(function(x)x^pow[i], xlim=c(0 + (i-1)*1, 1 + (i-1)*1), add=T)
}
pow  <- c(1,2, 3, 4)
